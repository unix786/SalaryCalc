  <data name="dbo_t_RecalcSalaryTest_TestAction.SqlScript" xml:space="preserve">
    <value>-- database unit test for dbo.t_RecalcSalary
DECLARE @Name AS [dbo].[Name_t], @Employed AS [dbo].[Year_t], @PositionID AS [dbo].[Identity_t], @ManagerID AS [dbo].[Identity_t], @Rating AS TINYINT, @YearsEmployed AS TINYINT, @PrevRating1 AS TINYINT, @PrevRating2 AS TINYINT, @Salary AS INT
SELECT @Name = NULL,
       @Employed = NULL,
       @PositionID = NULL,
       @ManagerID = NULL,
       @Rating = NULL,
       @YearsEmployed = NULL,
       @PrevRating1 = NULL,
       @PrevRating2 = NULL,
       @Salary = 0

-- position lookup
SELECT @PositionID = ID FROM Position WHERE 1&lt;&gt;1
SELECT @PositionID
SELECT @PositionID = ID FROM Position WHERE Name = 'Technician'
SELECT @@ROWCOUNT

-- insert employee
SELECT
	@Name = 'unitTest',
	@Employed = 2014, -- doesn't really matter, because the server is unaware of current date.
	@YearsEmployed = 4,
	@Rating = 50, -- Scores are stored as integers, multiplied by 10.
	@PrevRating1 = 30, -- score for 2016
	@PrevRating2 = 10 -- score for 2015

-- The test will ignore "minimum wage" parameter, but the bonus table is assumed to have 15 % for a score of 4.
DECLARE @MinWage int
SELECT @MinWage = MinimumWage FROM Parameter
DECLARE @BaseSalary int
SELECT TOP 1 @BaseSalary = Value FROM BaseSalary WHERE PositionID = @PositionID AND Years &lt;= @YearsEmployed ORDER BY Years DESC
SELECT @BaseSalary -- Result set 3

INSERT Employee (Name, PositionID, Employed, YearsEmployed, Rating, PrevRating1, PrevRating2)
VALUES (@Name, @PositionID, @Employed, @YearsEmployed, @Rating, @PrevRating1, @PrevRating2)
DECLARE @EmployeeID Identity_t = @@IDENTITY

-- check calculation result
SELECT
	Salary - @MinWage,
	Salary - @MinWage + 800
FROM Employee
WHERE
	ID = @EmployeeID
	AND Name = @Name
	AND PositionID = @PositionID
	AND Employed = @Employed
	AND YearsEmployed = @YearsEmployed
	AND Rating = @Rating
	AND PrevRating1 = @PrevRating1
	AND PrevRating2 = @PrevRating2

-- delete created employee
DELETE Employee WHERE ID = @EmployeeID
SELECT * FROM Employee WHERE ID = @EmployeeID</value>
